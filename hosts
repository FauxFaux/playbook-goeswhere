#!/usr/bin/python3

import json


def hosts():

    ret = {
        '_meta': {'hostvars': {}},
    }

    for role in ['base', 'phab', 'prom']:
        ret[role] = {'hosts': ['blind.goeswhere.com']}

    return ret


def main():
    import sys
    json.dump(hosts(), sys.stdout)


if '__main__' == __name__:
    main()
